{
  "name": "diplom-project",
  "version": "0.0.1",
  "description": "",
  "author": "VladGoogle <vladittrello@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:down": "docker compose down",
    "docker:downVolumes": "docker compose down && docker volume prune",
    "docker:up": "npm run docker:down && docker compose up --build client",
    "copy:templates": "cpx 'backend/src/templates/**' 'dist/src/templates'",
    "build:server": "nest build",
    "format": "prettier --write \\\"backend/src/**/*.ts\\\" \\\"test/**/*.ts\\\" \\\"client/**/**/*.js\\\"",
    "start": "nest start",
    "start:test": "nest start --watch",
    "start:server:dev": "yarn migration:dev && prisma db seed && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/backend/src/main.js",
    "migration:dev": "yarn prisma migrate dev",
    "migration:reset": "yarn prisma migrate reset --force",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "watch": "npm-watch",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "db:restart": "docker compose down && docker compose up -d  test-server && sleep 1",
    "pretest:int": "yarn db:restart",
    "test:int": "dotenv -e .env.test -- jest -i --no-cache --watch --config jest-e2e.json",
    "start:client": "react-app-rewired start",
    "build:client": "react-app-rewired build",
    "test:client": "react-scripts test",
    "eject:client": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hapi/joi": "^17.1.1",
    "@hookform/resolvers": "^3.1.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/cli": "^9.4.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.0",
    "@nestjs/websockets": "^9.4.0",
    "@prisma/client": "^4.9.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@stripe/stripe-js": "^1.52.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/uuid": "^9.0.1",
    "aws-sdk": "^2.1336.0",
    "aws-sdk-js-codemod": "^0.14.1",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cpx": "^1.5.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "jest-mock-extended": "^3.0.4",
    "joi": "^17.8.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nodemailer": "^6.9.1",
    "npm-watch": "^0.11.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.29.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^11.17.0",
    "swiper": "^9.2.0",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.1.1",
    "zxcvbn": "^4.4.2"
  },
  "overrides": {
    "typescript": "^5.0.2"
  },
  "watch": {
    "start:client": {
      "patterns": [
        "client/src"
      ],
      "extensions": "js,jsx"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.7",
    "prisma": "^4.9.0",
    "react-app-rewired": "^2.2.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "webpack": "^5.79.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "backend/src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "main": "index.js",
  "repository": "https://github.com/VladGoogle/diplom_project.git"
}
